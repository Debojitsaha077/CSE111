import java.util.Scanner;
public class task3{
  public static void main(String()args){
    Scanner sc = new Scanner(System.in);
    
    String s1= sc.nextLine();
    String s2= sc.nextLine();
    
    String s3 = s1 + " " + s2;
    
    int sum;
    
    sum= (int)s3;
    
    System.out.println(s3);
    System.out.println(sum);
  }
}